{
  "manifest_version": 3,
  "name": "Innotate New",
  "version": "2.0",
  "description": "Making the world fall in ❤️ with online reading...",
  "web_accessible_resources": [
    {
      "resources": [
        "note.css",
        "images/*",
        "utils/*",
        "Methods/*",
        "component/*",
        "data_Model/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "note.css"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "storage",
    "contextMenus",
    "tabs"
  ]
}